class Solution(object):
    def arrayStringsAreEqual(self, word1, word2):
        """
        :type word1: List[str]
        :type word2: List[str]
        :rtype: bool
        """
        res1 =''
        res2 =''
        for i in word1:
            res1=res1+i
        for j in word2:
            res2=res2+j
        return res1==res2
       
