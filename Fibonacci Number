class Solution(object):
    def fib(self, n):
        """
        :type n: int
        :rtype: int
        """
        if(n==0):
            return 0
        elif (n==1):
            return 1
        a = 0
        b = 1
        c = 0 
        i = 1
        while i<n:
            c=a+b
            a=b
            b=c
            i=i+1
        return c;
